<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:overScrollMode="never"
    android:scrollbars="none">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="15.0dip">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/more_type_title_height"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10.0dip"
            android:gravity="center_vertical"
            android:text="@string/more_content"
            android:textColor="@color/nba_medium_grey"
            android:textSize="12.0sp" />

        <RelativeLayout
            android:id="@+id/ly_team"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector">

            <ImageView
                android:id="@+id/icon_team"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_content_team" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_team"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/title_team"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ly_player"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector">

            <ImageView
                android:id="@+id/icon_player"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_content_player" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_player"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/title_player"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@id/icon_player"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ly_qdsc"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector">

            <ImageView
                android:id="@+id/icon_game"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_content_qdsc" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_game"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/title_qdsc"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@id/icon_game"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ly_ranking"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector">

            <ImageView
                android:id="@+id/icon_rank"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_content_ranking" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_rank"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/title_ranking"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@id/icon_rank"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ly_playoff_rank"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector"
            android:visibility="gone">

            <ImageView
                android:id="@+id/icon_playoff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_iconcontent__jhsdz" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_playoff"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/title_playoff_rank"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@id/icon_playoff"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ly_picture"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector"
            android:visibility="gone">

            <ImageView
                android:id="@+id/icon_pic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_content_img" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_pic"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/title_picture"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@id/icon_pic"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ly_pics"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/icon_pics"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_content_img" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_pics"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/title_pics"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />

            <View
                android:id="@+id/ln_pics"
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@id/icon_pics"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ly_date"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/icon_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_content_time" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_date"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/title_date"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />

            <View
                android:id="@+id/ln_date"
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@id/icon_date"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ly_pushMsg"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector"
            android:visibility="gone">

            <ImageView
                android:id="@+id/icon_push"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_content_msg" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_push"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/title_push_msg"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />

            <View
                android:id="@+id/ln_push"
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@id/icon_push"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/common_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:background="@color/nba_light_grey" />

        <TextView
            android:id="@+id/tv_type_game"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/more_type_title_height"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10.0dip"
            android:gravity="center_vertical"
            android:text="@string/more_game"
            android:textColor="@color/nba_medium_grey"
            android:textSize="12.0sp"
            android:visibility="gone" />

        <RelativeLayout
            android:id="@+id/ly_nba"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector"
            android:orientation="horizontal"
            android:visibility="gone">

            <ImageView
                android:id="@+id/icon_nba_day"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_today_nba" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:src="@drawable/more_list_button_inter" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dip"
                android:layout_alignParentBottom="true"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/more_type_title_height"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10.0dip"
            android:gravity="center_vertical"
            android:text="@string/more_buy"
            android:textColor="@color/nba_medium_grey"
            android:textSize="12.0sp" />

        <RelativeLayout
            android:id="@+id/rly_shop"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="15.0dip"
                android:src="@drawable/more_icon_nbastore" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:src="@drawable/more_list_button_inter" />
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_alignParentBottom="true"
            android:background="@color/nba_light_grey" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/more_type_title_height"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10.0dip"
            android:gravity="center_vertical"
            android:text="@string/more_setting"
            android:textColor="@color/nba_medium_grey"
            android:textSize="12.0sp"
            android:visibility="gone" />

        <RelativeLayout
            android:id="@+id/ly_setting"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector"
            android:orientation="horizontal"
            android:visibility="gone">

            <ImageView
                android:id="@+id/icon_setting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_set" />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toRightOf="@id/icon_setting"
                android:drawableRight="@drawable/more_list_button_inter"
                android:text="@string/more_setting"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ly_clean"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/tab_more_item_height"
            android:background="@drawable/item_bg_selector"
            android:visibility="gone">

            <ImageView
                android:id="@+id/icon_clean"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginLeft="15.0dip"
                android:layout_marginRight="15.0dip"
                android:src="@drawable/more_icon_set_clear" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/icon_clean"
                android:text="@string/more_cleaner"
                android:textColor="@color/nba_black"
                android:textSize="16.0sp" />

            <TextView
                android:id="@+id/tv_cache_size"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:textColor="@color/nba_medium_grey"
                android:textSize="16.0sp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@id/icon_clean"
                android:background="@color/nba_light_grey" />
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1.0dip"
            android:layout_alignParentBottom="true"
            android:background="@color/nba_light_grey" />
    </LinearLayout>
</ScrollView>